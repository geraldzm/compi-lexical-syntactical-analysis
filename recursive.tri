let  recursive    proc Ping (x : Integer) ~ if x > 0 then Impr ('I') ; Pong (x - 1) else nothing end    end    and    proc Pong (a : Integer) ~ if a > 0 then Impr ('0') ; Ping (a - 1) else nothing end    end    and    proc Impr (c : Char) ~ put (c)    end  endin  Ping (6) ! imprime IOIOIOend